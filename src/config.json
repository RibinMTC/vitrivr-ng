{
  "api": {
    "host" : "localhost",
    "port" : 4567
  },
  "resources": {
    "host_thumbnails" : "http://localhost:4567/thumbnails/:s",
    "host_objects": "http://localhost:4567/objects/:o"
  },
  "competition": {
    "teamid": 2,
    "toolid": 1,
    "sessionid": "yoursessionid",
    "endpoint": "https://interactivevideoretrieval.com/",
    "log": false,
    "lsc": false,
    "vbs": false,
    "dres": false
  },
  "mlt": {
    "VIDEO": ["scenecaption", "localfeatures", "objectInstances"],
    "IMAGE": ["scenecaption", "localfeatures", "objectInstances"]
  },
  "query": {
    "history": 0,
    "options": {
      "image": true,
      "audio": false,
      "model3d": false,
      "motion": false,
      "text": true,
      "tag": true,
      "semantic": true,
      "boolean": true
    },
    "text": {
      "categories": [
        ["ocr", "Text on Screen"],
        ["asr", "Audio"],
        ["scenecaption", "Description"]
      ]
    },
    "boolean": [
      ["Aesthetic Score", "RANGE", "features_aestheticscore.feature", 2, 8]
    ],
    "staged": true
  },
  "refinement": {
    "filters": [
      ["dominantcolor.color", "CHECKBOX"],
      ["technical.duration", "SLIDER"],
      ["LSCMETA.semantic_name", "CHECKBOX"]
    ]
  }
}
