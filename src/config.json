{
  "api": {
    "host" : "default",
    "port" : 4567
  },
  "resources": {
    "host_thumbnails" : "http://129.132.62.166:4567/thumbnails/:s",
    "host_objects": "http://129.132.62.166:4567/objects/:o"
  },
  "aestheticConfig": {
    "endpoint": "http://localhost:5003/aesthetic_details/"
  },
  "competition": {
    "teamid": 2,
    "toolid": 1,
    "endpoint": "https://interactivevideoretrieval.com/",
    "log": false,
    "lsc": false,
    "vbs": false,
    "dres": false
  },
  "mlt": {
    "VIDEO": ["scenecaption", "localfeatures", "objectInstances"],
    "IMAGE": ["scenecaption", "localfeatures", "objectInstances"]
  },
  "query": {
    "history": 0,
    "options": {
      "image": true,
      "audio": false,
      "model3d": false,
      "motion": false,
      "text": true,
      "tag": true,
      "semantic": true,
      "boolean": true
    },
    "text": {
      "categories": [
        ["ocr", "Text on Screen"],
        ["asr", "Audio"],
        ["scenecaption", "Description"]
      ]
    },
    "boolean": [
      ["Hecate Brightness", "RANGE", "features_hecate_image_metrics.brightness", 0, 1, 0.1],
      ["Hecate Sharpness", "RANGE", "features_hecate_image_metrics.sharpness", 0, 1, 0.1],
      ["Hecate Asymmetry", "RANGE", "features_hecate_image_metrics.asymmetry", 0, 1, 0.1],
      ["Aesthetic Score", "RANGE", "features_aesthetic_score.aestheticScore", 2, 8, 1],
      ["Facial Emotion", "OPTIONS", "features_facial_emotion.facialEmotions", "angry", "fear", "sad", "neutral", "none", "happy", "surprise"]
    ],
    "staged": true
  },
  "refinement": {
    "filters": [
      ["dominantcolor.color", "CHECKBOX"],
      ["technical.duration", "SLIDER"]
    ]
  }
}
